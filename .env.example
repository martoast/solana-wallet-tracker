# Solana RPC Configuration
SOLANA_RPC_HTTP=https://mainnet.helius-rpc.com/?api-key=YOUR_KEY_HERE
SOLANA_RPC_WS=wss://mainnet.helius-rpc.com/?api-key=YOUR_KEY_HERE

# For testing with free RPC (not recommended for production):
# SOLANA_RPC_HTTP=https://api.mainnet-beta.solana.com
# SOLANA_RPC_WS=wss://api.mainnet-beta.solana.com

# Wallet addresses to track (comma-separated)
TRACKED_WALLETS=FCSsZQNhA9LmDW9D4tcLXgi4BkY3Bz8kjUqTYoNTaqu9

# Jupiter Ultra API (optional - only needed if you want to use paid tier)
# JUPITER_API_KEY=your_api_key_here

# Minimum swap value to display (in USD)
MIN_SWAP_VALUE_USD=1

# Performance Tracking (true/false)
# Track P&L, win rate, and portfolio performance
TRACK_PERFORMANCE=true

# Show Only Token Swaps (true/false)
# Filter out non-trading transactions (transfers, etc)
# Focus only on SOL <-> Token swaps for cleaner output
SHOW_ONLY_TOKEN_SWAPS=true